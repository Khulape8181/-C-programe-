//write code for multiplication of two numbers 
//solution:-
 #include<stdio.h>
int main ()
{
  int r=60;
  int s =15;
  int multi=0;
  
  multi= r*s;
  
  printf("r value=%d\n",r);
  printf("s value =%d\n",s);
  printf("multi value=%d\n",multi );
  
  printf("r address=%d\n",&r);
  printf("s address=%d\n",&s);
  printf("multi address=%d\n",&multi );
  return 0;
}
